{
    "extends": "eslint-config-rallycoding",
    "rules": {
        "arrow-body-style": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "max-len": [1, 200, 2, {
          "ignorePattern": "^import\\s.+\\sfrom\\s.+;$",
          "ignoreUrls": true
        }]
      },
      "globals": { "fetch": false }
}